       to_char(ticketdate, 'DD-MM-YYYY') as "fecha",
       to_char(tickethour, 'HH24:MI') as "hora",
       case
       when (state='V') then 'Vigente'
       else 'Expiro'
       end as "estado",
       concat('$', totaltotal) as "total"
